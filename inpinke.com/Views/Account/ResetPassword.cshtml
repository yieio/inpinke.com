@model inpinke.com.Models.ResetPasswordModel
@{
    ViewBag.Title = "重设密码";
}
<div class="content_wrapper reg_wrapper">
    <p class="reg_title">
        <span>重设密码</span>
    </p>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    @using (Html.BeginForm("resetpassword", "account", FormMethod.Post, new { @class = "account_form" }))
    {
        <div>
            <div class="editor_label">
                @Html.LabelFor(m => m.Email)
            </div>
            <div class="editor_field">
                @Html.TextBoxFor(m => m.Email)
                @Html.ValidationMessageFor(m => m.Email)
            </div>
            <p class=" editor_label btn_field">
                <input type="submit" value="重设密码" class="color_btn" />
            </p>
        </div>        
    }
    <div class="login_tip">
        <p>
            >>还没有印品客账号？<a href="/account/register" style="color: #007700;">立即注册</a></p>
        <p>
            >>其他方式登录：</p>
        <p>
            <a href="javascript:;" title="" class="icon_bg weibo_icon">新浪微博登录</a> <a href="javascript:;"
                title="" class="icon_bg qq_icon">QQ账号登录</a> <a href="javascript:;" title="" class="icon_bg douban_icon">
                    豆瓣账号登录</a>
        </p>
    </div>
</div>
