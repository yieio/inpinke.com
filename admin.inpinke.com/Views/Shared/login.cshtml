@{
    Layout = null;
}
<!DOCTYPE html>
<html style="background: #f6f6f6;">
<head>
    <title>Inpinke-印品客用户管理后台</title>
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico")" />
    <link href="@Url.Content("~/Content/site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery-ui-1.8.11.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cookie.js")" type="text/javascript"></script>
</head>
<body style="background: #f6f6f6;">
    <div class="login_top">
        <img src="../../Content/images/admin_top_logo1.png" alt="" />
    </div>
    <form class="login_form" action="/login?f=@(ViewBag.ReturnUrl)" method="post">
    <table cellpadding="0" cellspacing="0">
        <tbody>
            <tr>
                <th>
                    用户名：
                </th>
                <td>
                    <input type="text" class="form_input_txt" name="username" />
                </td>
            </tr>
            <tr>
                <th>
                    密码：
                </th>
                <td>
                    <input type="text" class="form_input_txt" name="password" />
                </td>
            </tr>
        </tbody>
        <tfoot>
        <tr>
            <th></th>
                <td style=" color:#f00">
                    @(ViewBag.Msg)
                </td>
            </tr>
            <tr>
            <th></th>
                <td>
                    <input type="submit" value="登录" class="color_btn" />
                </td>
            </tr>
        </tfoot>
    </table>
    </form>
</body>
</html>
